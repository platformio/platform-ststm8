{
  "build": {
    "core": "sduino",
    "extra_flags": "-DSTM8S_UNO -DSTM8S105",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "stm8s105k6t6",
    "variant": "s8uno"
  },
  "debug": {
    "svd_path": "STM8S105K6.svd",
    "openocd_target": "stm8s105"
  },
  "frameworks": [
    "arduino",
    "spl"
  ],
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 32768,
    "protocol": "serial",
    "protocols": [
      "stlinkv2",
      "serial"
    ]
  },
  "name": "sduino UNO (STM8S105K6)",
  "url": "https://github.com/roybaer/sduino_uno",
  "vendor": "sduino"
}
